      // Fade out buttons when opening menu
      await fadeOutButtons(root);

      await revealMenu(menu);
      s.openMenuEl = menu;
      s.openTrigger = trigger || null;
      if (s.openTrigger) s.openTrigger.setAttribute("aria-expanded", "true");

