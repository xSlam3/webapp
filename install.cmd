      });
      // Animate heading with split text
      if (heading) {
        const split = SplitText.create(heading.children, { 
          type: "words",
          wordsClass: "word",
        });
        tl.from(split.words, {
          autoAlpha: 0,
          duration: 1,
          ease: "power2.out",
          stagger: { amount: 0.2 }
        });
      }
      // Animate text
      if (text) {
        tl.from(text, {
          autoAlpha: 0,
          y: 10,
          duration: 0.75,
          ease: "power2.out"
        }, "<+30%");
      }
      // Animate CTA
      if (cta) {
        tl.from(cta, {
          autoAlpha: 0,
          y: 10,
          duration: 0.75,
